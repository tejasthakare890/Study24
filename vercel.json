{
  "routes": [
    { "src": "/login", "dest": "/pages/login.js" },
    { "src": "/signup", "dest": "/pages/signup.js" },
    { "src": "/api/login", "dest": "/api/login.js" },
    { "src": "/api/signup", "dest": "/api/signup.js" },
    { "src": "/api/*", "dest": "/api/index.js" },
    { "src": "/(.*)", "dest": "/pages/$1" }
  ]
}
